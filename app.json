{
  "expo": {
    "name": "SplitSnap",
    "slug": "splitsnap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "splitsnap",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1B1B1F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.splitsnap.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#1B1B1F"
      },
      "package": "com.splitsnap.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow SplitSnap to scan receipts"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
